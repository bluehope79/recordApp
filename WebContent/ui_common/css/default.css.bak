section.sch a.btn_sch {float:right; width:88px; height:60px; border:2px solid #ab3c6f; }
section.sch a.btn_sch span {display:inline-block; margin-top:10px; font-size:0; text-indent:-9999px;width:40px; height:40px; color:rgba(255,255,255,0); background:url(../img/ico_sch.png) no-repeat 0 0;background-size:cover;vertical-align:top;}
section.sch a.btn_sch:active {background:#ab3c6f; }
section.sch a.btn_sch:active span {background-position:0 -40px;}

.sch + .sch_result {}
.listWrap {position:relative;}

.tbBox { position:relative; margin-top:50px; height:510px; border-bottom:1px solid #b3b3b3; background:rgba(200,200,200,0.1);}
.tbBox table {position:relative; top:-40px;}
.tbBox table td {height:50px; letter-spacing:0;}
