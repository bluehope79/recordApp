/* style */

.btn {display:block; text-align:center; border-radius:5px; font-size:1em; letter-spacing:0;}
.btn span {}
.btn_icon span {display:none;}

.btn_login { height:70px; line-height:66px; font-size:1.625em; color:#fff; letter-spacing:0; background:#5a5387; border-radius:10px;}
.btn_agree { width:240px; height:65px; font-size:1.25em; line-height:60px; color:#fff; background-color:#a8356a;}

.btnPlayer {text-align:center;}
.btnPlayer a {display:inline-block; text-align:center; width:52px; height:52px; border-radius:30px; border:2px solid #999; margin:0 10px;}
.btnPlayer a:before {content:''; display:inline-block; width:30px; height:30px; margin:11px auto 0; background:url(../img/ico_btnPlayer.png) no-repeat 0 0;  }
.btnPlayer a.ac {border-color: #191919;}
.btnPlayer .btn_play {width:70px; height:70px; border-radius:40px;}
.btnPlayer .btn_pause:before  {background-position: 0 -60px;}
.btnPlayer .btn_play:before  {background-position: -30px -60px; margin-top:20px; }
.btnPlayer .btn_stop:before {background-position: -60px -60px;}
.btnPlayer .btn_pause.ac:before  {background-position: 0 0;}
.btnPlayer .btn_play.ac:before  {background-position: -30px 0; margin-top:20px; }
.btnPlayer .btn_stop.ac:before {background-position: -60px 0;}



table.tb_set {}
.tb_set th {fon-size:1em; }
.tb_set tb {fon-size:1em; }

.tb_row th {height:40px;text-align:center; color:#a8356a; border-bottom:1px solid #a9a9a9; }
.tb_row td {text-align:center; border-bottom:1px solid #e7e8ea; }

.progWrap {width:520px; text-align:center; color:#fff; font-size:1em;}
.progWrap .loading {position:relative; }
/*
.progWrap .loading:before {content:''; display:block; margin:0 auto; width:60px; height:30px; margin-bottom:5px; background:url(../img/ico_loading.png) no-repeat; }
*/

.loading .loadingImg {margin:0 auto; width:65px; height:40px; vertical-align:bottom;}
.loading .loadingImg span {display:-block; float:left; width:4px; height:30px; margin-right:3px; vertical-align:bottom; }
.loading .loadingImg span:last-child {margin-right:0;}
.loading .loadingImg span:before {content:''; display:inline-block; width:4px; height:30px; background-color:#fff; vertical-align:middle; border-radius:3px;}
.loading .loadingImg span:nth-child(1):before {height:5px; -webkit-animation: loader1 3s linear infinite;animation: loader1 3s linear infinite;}
.loading .loadingImg span:nth-child(2):before {height:7px; -webkit-animation: loader2 3s linear infinite;animation: loader2 3s linear infinite;}
.loading .loadingImg span:nth-child(3):before {height:10px; -webkit-animation: loader3 3s linear infinite;animation: loader3 3s linear infinite;}
.loading .loadingImg span:nth-child(4):before {height:6px; -webkit-animation: loader4 3s linear infinite;animation: loader4 3s linear infinite;}
.loading .loadingImg span:nth-child(5):before {height:20px; -webkit-animation: loader5 3s linear infinite;animation: loader5 3s linear infinite;}
.loading .loadingImg span:nth-child(6):before {height:29px; -webkit-animation: loader6 3s linear infinite;animation: loader6 3s linear infinite;}
.loading .loadingImg span:nth-child(7):before {height:22px; -webkit-animation: loader7 3s linear infinite;animation: loader7 3s linear infinite;}
.loading .loadingImg span:nth-child(8):before {height:7px; -webkit-animation: loader8 3s linear infinite;animation: loader8 3s linear infinite;}


