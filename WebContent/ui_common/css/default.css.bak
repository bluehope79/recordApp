/* search list */
section.sch {}
section.sch h1 {padding-top:25px; font-size:2em;}
section.sch .schWrap {padding:15px 90px;border-bottom:1px solid #333333; }
section.sch .schWrap table tr:last-child td {border-top:1px dashed #d7d7d7;}
