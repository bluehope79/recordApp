.recordInsWrap .recordBar {position:absolute; width:320px; height:160px; border-right:2px solid #f6c500;}
.recordInsWrap .recordBar span {display:block; height:160px;background:#424165; opacity:0.3;}
.recordInsWrap .recordBar .cursor {position:absolute; left:0; top:-31px; width:16px; height:31px; background:url(../img/ico_cursor.png) no-repeat 0 0;}
.recordInsWrap .recordImg {position: absolute; width:640px; height:160px; background:url(../img/ico_record.png) repeat-x 0 50%; -webkit-animation: mymove 30s infinite;animation: mymove 30s infinite; -webkit-animation-timing-function: linear;animation-timing-function: linear;}
.recordInsWrap .recordImg2 {position: absolute; width:640px; height:160px; background:url(../img/ico_record.png) repeat-x 0 50%; -webkit-animation: mymove2 30s infinite;animation: mymove2 30s infinite; -webkit-animation-timing-function: linear; animation-timing-function: linear;}
.recordInsWrap.pause .recordImg {animation-play-state: paused;}
.recordInsWrap.pause .recordImg2 {animation-play-state: paused;}
@-webkit-keyframes mymove {
    from {right: -640px;}
    to {right: 0;}
}
@keyframes mymove {
    from {right: -640px;}
    to {right: 0;}
}
@-webkit-keyframes mymove2 {
    from {right: 0;}
    to {right: 640px;}
}
@keyframes mymove2 {
    from {right: 0;}
    to {right: 640px;}
}


/* popup */
.pop {position:absolute; top:0; left:0; width:100%; height:100%; background:rgba(0,0,0,0.7); z-index:1000;}
.pop .popWrap {position:absolute; }

.pop_audioF .popWrap {width:800px; height:540px;top:50%; left:320px; margin-left:80px; margin-top:-270px; background:#fff;}
.pop_audioF .pop_hd {position:relative; background:#5a5387; height:135px; padding:30px 40px 25px; color:#fff; font-size:1.25em;}
.pop_audioF .pop_hd a { color:#fff;}
.pop_audioF .txt_patient {font-size:1.2em; }
.pop_audioF .txt_patient em {font-size:1.1em; font-weight:bold;}
.pop_audioF .txt_file { position:absolute; bottom:25px; letter-spacing:0;}
.pop_audioF .txt_file:before {content:''; display:inline-block; width:22px; height:22px; vertical-align:middle; margin-right:8px; background:url(../img/ico_wav.png) no-repeat 0 0; background-size:cover;}
.pop_audioF a.file_download { position:absolute; bottom:25px; right:40px; letter-spacing:0;}
.pop_audioF a.file_download:before {content:''; display:block; position:absolute; left:-50px; top: -5px; width:42px; height:42px; background:url(../img/ico_down.png) no-repeat 0 0; background-size:cover;}
.pop_audioF .btn_close {position:absolute; top:-70px; left:50%; margin-left:-30px;width:60px; height:60px; background:url(../img/ico_delPop.png) no-repeat 50% 50%; background-size:cover;}

.audWrap {width:640px; height:100px; border-radius:20px; background-color:#f3f3f3; margin:65px auto 55px;}

.pop_start .popWrap {width:430px; height:270px; background:#fff; top:50%; left:50%;  margin-top:-135px; margin-left:-50px; text-align:center;}
.pop_start .txt {font-size:1.5em; color:#333; padding-top:75px; }
.pop_start .btnWrap {position:absolute;bottom:0;width:100%; background:#f7f7f7; border-top:1px solid #dedede; overflow:hidden;}
.pop_start .btnWrap .pop_btn {display:inline-block; height:80px; width:49.5%; font-size:1.5em; color:#333; float:left;}
.pop_start .btnWrap .pop_btn span {display:inline-block; padding:20px 0;}
.pop_start .btnWrap .btn_ok {color:#fff; background:#ff9393;}

