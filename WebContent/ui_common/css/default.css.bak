/* common */
html, body{height:100%;}

.container {height:100%; background:url(../img/bg_default.jpg) no-repeat; background-size:cover;overflow:hidden; }

article.content {margin-left:320px; }
article section {padding:0 60px;}
article section h1 {padding-top:48px; text-align:center; font-size:2.5em; color:#a8356a; font-weight:normal;}


/* header */
header.header {position:absolute; left:0; top:0; width:320px; height:100%;background:url(../img/bg_hd.jpg) no-repeat; background-size:cover; z-index:2000;}
.header .hd_logo {padding:0 40px; height:75px; border-bottom:1px solid rgba(255,255,255,0.3); }
.header .hd_logo a {display:inline-block; margin-top:25px; font-size:1.2em; line-height:1.625em; color:#fff; font-weight:bold; text-shadow:1px 1px 1px rgba(0,0,0,0.4);}
.header .hd_logo a:before {content:''; display:inline-block; width:30px; height:30px; margin-right:8px; background:url(../img/ico_logo.png) no-repeat 50% 50%; background-size:cover;vertical-align:middle; }
.header .hd_info {margin-top:40px; padding-left:40px; color:#fff; font-size:1.2em; text-shadow:1px 1px 1px rgba(0,0,0,0.3);}
.header .hd_info li {margin-bottom:18px; font-weight:bold;}

.header .hd_nav {position:absolute; bottom:40px; width:100%; height:40px; }
.header .hd_nav ul {margin-left:60px;  }
.header .hd_nav ul li {display:inline-block; margin-right:40px;}
.header .hd_nav ul li:last-child {margin-right:0;}
.header .hd_nav ul li a {display:inline-block; width:40px; height:40px;  background:url(../img/ico_nav.png) no-repeat 0 0; background-size:40px auto;}
.header .hd_nav ul li a span {text-indent:-999px; font-size:0; color:rgba(255,255,255,0);}
.header .hd_nav ul li.ico_custmer a {background-position:0 -40px;}
.header .hd_nav ul li.ico_help a {background-position:0 -80px;}


/* login */
.container.member { background:url(../img/bg_01.jpg) no-repeat; background-size:cover; overflow:hidden;}
.member header.header {height:auto; background:transparent; }
.member .header .hd_logo {position:absolute; border-bottom:0;}
.content .login {position:absolute; top:50%; left:50%; margin:-215px 0 0 -260px; padding:0; z-index:100;}
.content .loginWrap {position:relative; width:520px; height:430px; padding:40px 70px 35px; box-sizing:border-box; background:rgba(255,255,255,0.6); border:1px solid #fff; border-radius:18px; text-align:center;}
.content .progWrap {position:relative; margin-top:20px; }
.member .idxBgW {position:absolute; left:0; top:490px; width:100%; height:200px;z-index:50; overflow:hidden;}
.member .idxBgW:before {content:''; position:absolute;  display:block; width:1280px; height:200px; background:url(../img/idx_ptn01.png) no-repeat 0; background-size:auto 200px;-webkit-animation: idxBg1 80s infinite;animation: idxBg1 80s infinite; -webkit-animation-timing-function: linear;animation-timing-function: linear;}
.member .idxBgW:after {content:''; position:absolute;  display:block; width:1280px; height:200px; background:url(../img/idx_ptn01.png) no-repeat 0; background-size:auto 200px;-webkit-animation: idxBg102 80s infinite;animation: idxBg102 80s infinite; -webkit-animation-timing-function: linear;animation-timing-function: linear;}
@-webkit-keyframes idxBg1 {
    from {right: -1280px;}
    to {right: 0;}
}
@keyframes idxBg1 {
    from {right: -1280px;}
    to {right: 0;}
}
@-webkit-keyframes idxBg102 {
    from {right: 0;}
    to {right: 1280px;}
}
@keyframes idxBg102 {
    from {right: 0;}
    to {right: 1280px;}
}
.member .idxBg:before {content:''; position:absolute;  display:block; width:1280px; height:200px; background:url(../img/idx_ptn02.png) no-repeat 0; background-size:auto 200px;-webkit-animation: idxBg2 40s infinite;animation: idxBg2 40s infinite; -webkit-animation-timing-function: linear;animation-timing-function: linear;}
.member .idxBg:after {content:''; position:absolute;  display:block; width:1280px; height:200px; background:url(../img/idx_ptn02.png) no-repeat 0; background-size:auto 200px;-webkit-animation: idxBg202 40s infinite;animation: idxBg202 40s infinite; -webkit-animation-timing-function: linear;animation-timing-function: linear;}
@-webkit-keyframes idxBg2 {
    from {right: -1280px;}
    to {right: 0;}
}
@keyframes idxBg2 {
    from {right: -1280px;}
    to {right: 0;}
}
@-webkit-keyframes idxBg202 {
    from {right: 0;}
    to {right: 1280px;}
}
@keyframes idxBg202 {
    from {right: 0;}
    to {right: 1280px;}
}
.member .idxBg p:before {content:''; position:absolute;  display:block; width:1280px; height:200px; background:url(../img/idx_ptn03.png) no-repeat 0; background-size:auto 200px;-webkit-animation: idxBg3 53s infinite;animation: idxBg3 53s infinite; -webkit-animation-timing-function: linear;animation-timing-function: linear;}
.member .idxBg p:after {content:''; position:absolute;  display:block; width:1280px; height:200px; background:url(../img/idx_ptn03.png) no-repeat 0; background-size:auto 200px;-webkit-animation: idxBg302 53s infinite;animation: idxBg302 53s infinite; -webkit-animation-timing-function: linear;animation-timing-function: linear;}
@-webkit-keyframes idxBg3 {
    from {right: -1280px;}
    to {right: 0;}
}
@keyframes idxBg3 {
    from {right: -1280px;}
    to {right: 0;}
}
@-webkit-keyframes idxBg302 {
    from {right: 0;}
    to {right: 1280px;}
}
@keyframes idxBg302 {
    from {right: 0;}
    to {right: 1280px;}
}
.member .idxBgW {content:''; display:block; position:absolute; left:0; top:490px; width:100%;}


