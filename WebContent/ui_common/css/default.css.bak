.tb_paging {text-align:center; margin:10px 0;}
.tb_paging .pg_icon {width:32px; height:32px; background:url(../img/ico_tbPg.png) no-repeat 0 0; border-radius:5px;}
.tb_paging .pg_icon span {display:none;}
.tb_paging a {display:inline-block; line-height:30px; width:32px; text-align:center; font-size:0.895em; color:#777; vertical-align:middle; }
.tb_paging a.on,
.tb_paging a.ac, 
.tb_paging a:active{color: #e38060; font-weight:bold;}
.tb_paging .first {background-position:0 0;}
.tb_paging .before {background-position:-32px 0;margin-right:10px;}
.tb_paging .next {background-position:-64px 0; margin-left:10px;}
.tb_paging .last {background-position:-96px 0;}
.tb_paging .first:active {background-position:0 -32px; background-color:#bfbfbf;}
.tb_paging .before:active {background-position:-32px -32px; background-color:#bfbfbf;}
.tb_paging .next:active {background-position:-64px -32px; background-color:#bfbfbf;}
.tb_paging .last:active {background-position:-96px -32px; background-color:#bfbfbf;}

